<h1>Select the ingredients you already have ğŸ¤¤</h1>
<%= form_with url: pantry_ingredients_path, method: :get, class: "mb-3" do |form| %>
  <%= form.text_field :query,
                      class: "form-control search-bar",
                      placeholder: 'Search ingredient' %>
<% end %>
    <%= link_to 'meat', pantry_ingredients_path(category: 'meat') %>
    <%= link_to 'fish', pantry_ingredients_path(category: 'fish') %>
    <%= link_to 'dairy', pantry_ingredients_path(category: 'milk egg dairy cheese') %>
    <%= link_to 'jams', pantry_ingredients_path(category: 'nut butter jams honey') %>
    <%= link_to 'beverages', pantry_ingredients_path(category: 'beverage') %>
    <%= link_to 'Seasonings', pantry_ingredients_path(category: 'spice seasoning') %>
    <%= link_to 'Carbs', pantry_ingredients_path(category: 'pasta rice baking') %>
    <%= link_to 'Dressings', pantry_ingredients_path(category: 'Oil Vinegar Salad Dressing') %>
    <%= link_to 'canned', pantry_ingredients_path(category: 'can jar') %>
    <%= link_to 'frozen', pantry_ingredients_path(category: 'refrigerate frozen') %>
    <%= link_to 'produce', pantry_ingredients_path(category: 'fruit vegetable produce') %>
<div data-send-ingredients-target="list" data-controller="send-ingredients" data-ingredients-index-target="list"  data-send-ingredients-url-value="<%= pantry_pantry_ingredients_url %>">
  <%  @not_have_ingredients.each do |ingredient| %>
      <%= render "ingredient_card", ingredient: ingredient %>
  <% end %>
  <div>
    <button class="primary-dark-btn-wfd" data-action="click->send-ingredients#send">Add ingredients to your pantry</button>
  </div>
</div>
